# API Keys
OPENAI_API_KEY=your-openai-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here
GOOGLE_API_KEY=your-google-api-key-here

# Provider Settings
LLM_DEFAULT_PROVIDER=anthropic
LLM_FALLBACK_ENABLED=true
LLM_FALLBACK_PROVIDERS=openai,google
LLM_CACHE_ENABLED=true
LLM_CACHE_TTL=3600

# Cost Management
LLM_COST_TRACKING_ENABLED=true
LLM_DAILY_COST_LIMIT=100.0
LLM_MONTHLY_COST_LIMIT=2000.0
LLM_DAILY_COST_ALERT=100.00
LLM_MONTHLY_COST_ALERT=2000.00
LLM_COST_LOG_FILE=logs/llm_costs.log
LLM_DETAILED_LOGGING=true

# Performance Settings
LLM_REQUEST_TIMEOUT=60
LLM_MAX_RETRIES=3
LLM_RETRY_DELAY=1

# Rate Limits
OPENAI_RPM=500
OPENAI_TPM=90000
ANTHROPIC_RPM=50
ANTHROPIC_TPM=100000
GOOGLE_RPM=60
GOOGLE_TPM=60000

# Model Configuration
OPENAI_DEFAULT_MODEL=gpt-4-turbo-preview
ANTHROPIC_DEFAULT_MODEL=claude-3-sonnet-20240229
GOOGLE_DEFAULT_MODEL=gemini-pro

# Agent Configuration
AGENT_CONCURRENT_LIMIT=5
AGENT_TIMEOUT=300
AGENT_MEMORY_TTL=3600
AGENT_ASTRA_BASE_URL=https://api.agentastra.ai/v2
AGENT_ASTRA_TIMEOUT=120
AGENT_ASTRA_MAX_RETRIES=3